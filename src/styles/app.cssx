.card {
  background: ${ theme("#111", "#fff") };
  width: ${ percent(1, 3) };
  font-size: ${ clamp(14, 18, 24) }px;
  border: ${ once(() => rand(1, 5)) }px solid ${ darken("blue", 20) };
}

@media (max-width: 600px) {
  .card {
    font-size: ${ rem(0.9) };
  }
}

.once-test {
  border: ${ once(() => Math.random() * 100) }px solid blue;
}


